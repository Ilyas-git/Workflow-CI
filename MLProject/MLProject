name: stroke-prediction-mlflow

entry_points:
  main:
    parameters:
      data_path: {type: path, default: "Stroke_preprocessing.csv"}
      model: {type: str, default: "logreg"}
      tracking_dir: {type: str, default: "mlruns_ci"}
      experiment_name: {type: str, default: "stroke-prediction"}
    command: >
      python modelling.py
      --data_path {data_path}
      --model {model}
      --tracking_dir {tracking_dir}
